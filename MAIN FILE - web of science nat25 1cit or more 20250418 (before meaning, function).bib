@article{ WOS:000999672700007,
Author = {Athanassiou, Phoebus L.},
Title = {Central Bank Money as a Liability of its Issuer and the Case of Central
   Bank Digital Currencies},
Journal = {EUROPEAN LAW REVIEW},
Year = {2023},
Volume = {48},
Number = {2},
Month = {APR},
Abstract = {This article explores the validity of challenges to the traditional
   legal treatment of central bank money as a liability of its issuing
   central bank and assesses the implications of those challenges for the
   eventual issuance of central bank digital currencies. For the reasons
   explained below, the hitherto challenges to the regnant treatment of
   central bank money, whatever its form, as a liability of its issuing
   central bank appear to us unconvincing. Moreover, for the reasons
   explained in this article, these challenges could prove fatal to the
   prospect of the eventual issuance of central bank digital currencies
   under some of the main models currently under consideration by central
   banks across the world.},
