@article{ WOS:000312920400008,
Author = {Marimon, Ramon and Pablo Nicolini, Juan and Teles, Pedro},
Title = {Money is an experience good: Competition and trust in the private
   provision of money},
Journal = {JOURNAL OF MONETARY ECONOMICS},
Year = {2012},
Volume = {59},
Number = {8},
Pages = {815-825},
Month = {DEC},
Abstract = {The interplay between competition and trust as efficiency-enhancing
   mechanisms in the private provision of money is studied. With
   commitment, trust is automatically achieved and competition ensures
   efficiency. Without commitment, competition plays no role. Trust does
   play a role but requires a bound on efficiency. Stationary inflation
   must be non-negative and, therefore, the Friedman rule cannot be
   achieved. The quality of money can be observed only after its purchasing
   capacity is realized. In this sense, money is an experience good. (C)
   2012 Elsevier B.V. All rights reserved.},
