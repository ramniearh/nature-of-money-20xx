@article{ WOS:000083724200002,
Author = {Sethi, R},
Title = {Evolutionary stability and media of exchange},
Journal = {JOURNAL OF ECONOMIC BEHAVIOR \& ORGANIZATION},
Year = {1999},
Volume = {40},
Number = {3},
Pages = {233-254},
Month = {NOV},
Abstract = {The Kiyotaki and Wright model (1989) of money as a medium of exchange is
   analyzed within an evolutionary framework. It is shown that fundamental,
   speculative, and polymorphic states can all be stable, and that there
   may exist a multiplicity of stable states. Stringent assumptions
   regarding information and rationality are therefore unnecessary in order
   to explain the use of money as a medium of exchange, even when the
   existence of a stable medium requires some individuals to trade their
   production good for one with a higher storage cost, or when otherwise
   identical individuals must adopt different exchange strategies. (C) 1999
   Elsevier Science B.V. All rights reserved. JEL classification: C72; D83;
   E40.},
