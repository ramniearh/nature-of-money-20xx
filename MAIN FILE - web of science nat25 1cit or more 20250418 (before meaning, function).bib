@article{ WOS:000082041700003,
Author = {Cavalcanti, RD and Wallace, N},
Title = {Inside and outside money as alternative media of exchange},
Journal = {JOURNAL OF MONEY CREDIT AND BANKING},
Year = {1999},
Volume = {31},
Number = {3, 2},
Pages = {443-457},
Month = {AUG},
Abstract = {We study a random-matching model of money in which a subset of people,
   called bankers, have: known histories and the rest, called nonbankers,
   have unknown histories. Earlier, we showed that if there are no outside
   assets, then an optimal arrangement has bankers issuing objects,
   banknotes, that are used in trades involving nonbankers. Here, the same
   model is used to compare such exclusive use of inside money to the
   exclusive use of outside money. We show that the set of implementable
   outcomes using outside money is a strict subset of the set using inside
   money.},
