@article{ WOS:A1989U734600010,
Author = {WEBLEY, P and WILSON, R},
Title = {SOCIAL RELATIONSHIPS AND THE UNACCEPTABILITY OF MONEY AS A GIFT},
Journal = {JOURNAL OF SOCIAL PSYCHOLOGY},
Year = {1989},
Volume = {129},
Number = {1},
Pages = {85-91},
Month = {FEB},
Abstract = {We investigated the unacceptability of money as a gift in relationships
   varying in relative status and intimacy. Students were asked to specify
   how much they would spend on a present or gift money for a person in a
   particular relationship and to rank in order of acceptability a list of
   possible gifts for that person. Money was found to be generally
   unacceptable as a present no matter what the relationship. For intimate
   relationships, more was spent on money than on a gift when the receiver
   had  higher status than the giver, and less was spent on money when the
   giver had higher status. Overall, intimacy dictated the amount spent on
   a present, whereas relative status had more effect on the
   unacceptability of money as a gift.},
