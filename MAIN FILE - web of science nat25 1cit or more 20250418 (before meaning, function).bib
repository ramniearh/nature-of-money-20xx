@article{ WOS:000377461700004,
Author = {Smit, J. P. and Buekens, Filip and Du Plessis, Stan},
Title = {Cigarettes, dollars and bitcoins - an essay on the ontology of money},
Journal = {JOURNAL OF INSTITUTIONAL ECONOMICS},
Year = {2016},
Volume = {12},
Number = {2},
Pages = {327-347},
Month = {JUN},
Abstract = {What does being money consist in? We argue that something is money if,
   and only if, it is typically acquired in order to realise the reduction
   in transaction costs that accrues in virtue of agents coordinating on
   acquiring the same thing when deciding what thing to acquire in order to
   exchange. What kinds of things can be money? We argue against the common
   view that a variety of things (notes, coins, gold, cigarettes, etc.) can
   be money. All monetary systems are best interpreted as implementing the
   same basic protocol. Money, i.e. the thing that we coordinate on
   acquiring in order to lower our transaction costs, is, in all cases, a
   set of positions on an abstract mathematical object, namely a relative
   ratio scale. The things that we ordinarily call `money' are merely
   records of positions on such a scale.},
