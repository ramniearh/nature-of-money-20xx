@article{ WOS:000360963300009,
Author = {Oswiecimka, Pawel and Drozdz, Stanislaw and Gebarowski, Robert and
   Gorski, Andrzej Z. and Kwapien, Jaroslaw},
Title = {MULTISCALING EDGE EFFECTS IN AN AGENT-BASED MONEY EMERGENCE MODEL},
Journal = {ACTA PHYSICA POLONICA B},
Year = {2015},
Volume = {46},
Number = {8},
Pages = {1579-1592},
Month = {AUG},
Abstract = {An agent-based computational economical toy model for the emergence of
   money from the initial barter trading, inspired by Menger's postulate
   that money can spontaneously emerge in a commodity exchange economy, is
   extensively studied. The model considered, while manageable, is
   significantly complex, however. It is already able to reveal phenomena
   that can be interpreted as an emergence and collapse of money as well as
   the related competition effects. In particular, it is shown that as an
   extra emerging effect - the money lifetimes near the critical threshold
   value develop multiscaling, which allow one to set parallels to critical
   phenomena and, thus, to the real financial markets.},
