@article{ WOS:000451832200007,
Author = {Bridel, Pascal},
Title = {On the origin of money, or Menger's one-sided reading of Genovesi's
   Lezioni},
Journal = {EUROPEAN JOURNAL OF THE HISTORY OF ECONOMIC THOUGHT},
Year = {2018},
Volume = {25},
Number = {4, SI},
Pages = {627-636},
Abstract = {By examining Menger's interpretation of Genovesi's arguments on the
   origin of money, this note shows that Genovesi is a subtle theorist
   managing to blend the Cartalist with the Mengerian approaches to the
   origins of money. Far from resting exclusively on the Mengerian
   unattended consequences of the uncoordinated behaviours of rational
   agents trying to minimise their transaction costs, Genovesi shows how
   governments can and do create fiat money with a positive value in
   connexion with their ability to raise taxes. For Genvesi both trust (a
   la Menger) and authority (a la Cartalist) are necessary to explain the
   positive value of money.},
