@article{ WOS:000336212400006,
Author = {Semenova, Alla},
Title = {Carl Menger's theory of money's origins: Responding to revisionism},
Journal = {EUROPEAN JOURNAL OF THE HISTORY OF ECONOMIC THOUGHT},
Year = {2014},
Volume = {21},
Number = {1},
Pages = {107-141},
Abstract = {This article disputes the validity of the revisionist accounts of Carl
   Menger's theory of money's origins. While Menger's monetary thought
   underwent a complicated process of development and his later works
   reflected new Chartalist insights, Menger never escaped the theoretical
   framework of the spontaneous origins of the monetary unit, devoid of any
   action or intervention by the state. The role of the state in Menger's
   monetary theory was always confined to the later stages of money's
   development. While Menger once admitted a possibility of money's origins
   by the influence of public authority, he never incorporated it into his
   overall theoretical framework.},
