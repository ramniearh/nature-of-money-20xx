Author = {Lefebvre, Germain and Nioche, Aurelien and Bourgeois-Gironde, Sacha and
   Palminteri, Stefano},
Title = {Contrasting temporal difference and opportunity cost reinforcement
   learning in an empirical money-emergence paradigm},
